<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AirSim\Bundle\SocialNetworkBundle\Entity\Chat" table="chat">
    <id name="chatId" type="integer" column="chat_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="750" nullable="true"/>
    <field name="dateTimeCreated" type="datetime" column="date_time_created" nullable="false"/>
  </entity>
</doctrine-mapping>
